program ec1;
	read a;
	read b;
	read c;
	x = (c - b) / a;
	writeln x;

program ec2;
	read a;
	read b;
	read c;
	d = b * b - 4 * a * c;
	if d < 0 begin
		writeln "Ecuatia nu are radacini reale.";
	else begin
		if d == 0 begin
			x = (0 - b) / (2 * a);
			writeln x;
		else begin
			x1 = ((0 - b)  + sqrt(d)) / (2 * a);
			x2 = ((0 - b)  - sqrt(d)) / (2 * a);
			writeln x1;
			writeln x2;
		end;
	end;
